{
  "name": "@salesforce/command",
  "version": "4.2.2",
  "description": "Salesforce CLI base command class",
  "main": "lib/exported",
  "repository": "salesforcecli/command",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "prepack": "sf-build",
    "prepare": "npm run build",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "ci-docs": "yarn sf-ci-docs"
  },
  "keywords": [
    "force",
    "salesforce",
    "sfdx",
    "salesforcedx",
    "sfdx-command"
  ],
  "files": [
    "lib",
    "messages",
    "!lib/**/*.map"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "sf-husky-commit-msg",
      "pre-commit": "sf-husky-pre-commit",
      "pre-push": "sf-husky-pre-push"
    }
  },
  "dependencies": {
    "@oclif/command": "^1.8.1",
    "@oclif/errors": "^1.2.2",
    "@oclif/parser": "3.8.6",
    "@oclif/plugin-help": "^2.2.0",
    "@oclif/test": "^1.2.4",
    "@salesforce/core": "^2.35.0",
    "@salesforce/kit": "^1.5.17",
    "@salesforce/ts-types": "^1.5.20",
    "chalk": "^2.4.2",
    "cli-ux": "^4.9.3"
  },
  "devDependencies": {
    "@oclif/config": "^1.17.1",
    "@salesforce/dev-config": "^2.0.0",
    "@salesforce/dev-scripts": "^0.9.18",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.26.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-license": "^0.1.0",
    "eslint-config-salesforce-typescript": "^0.2.7",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^27.0.3",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "4.3.8",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "shelljs": "~0.8.3",
    "sinon": "^9.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^4.4.4"
  },
  "types": "lib/exported.d.ts",
  "publishConfig": {
    "access": "public"
  }
