    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "^35.1.2",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^7.0.4",
    "lodash-cli": "^4.17.5",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.0",
    "sinon": "10.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.3"
  }
